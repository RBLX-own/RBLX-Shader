    info.ZIndex = 2
    info.Parent = popup
    
    local ok = Instance.new("TextButton")
    ok.Size = UDim2.new(0, 90, 0, 40)
    ok.Position = UDim2.new(0.5, -45, 1, -55)
    ok.BackgroundColor3 = Color3.fromRGB(0, 255, 150)
    ok.TextColor3 = Color3.fromRGB(0, 0, 0)
    ok.Text = "Continue"
    ok.Font = Enum.Font.GothamBold
    ok.TextSize = 18
    ok.ZIndex = 3
    ok.Parent = popup
    
    local okCorner = Instance.new("UICorner")
    okCorner.CornerRadius = UDim.new(0, 10)
    okCorner.Parent = ok
    
    -- Open Telegram Link
    local function openTelegram()
        local url = "https://t.me/RBLX_SCRIPTS_MYANMAR"
        if setclipboard then
            setclipboard(url)
        end
        -- Try to open in browser for executors
        pcall(function()
            if syn and syn.request then
                syn.request({Url = url, Method = "GET"})
            elseif http and http.request then
                http.request({Url = url, Method = "GET"})
            elseif http_request then
                http_request({Url = url, Method = "GET"})
            elseif fluxus and fluxus.request then
                fluxus.request({Url = url})
            end
        end)
        linkBox.Text = "Opened/Copied! ‚úÖ"
        task.wait(2)
        linkBox.Text = "t.me/RBLX_SCRIPTS_MYANMAR"
    end
    
    linkBox.MouseButton1Click:Connect(openTelegram)
    
    -- Continue Button
    ok.MouseButton1Click:Connect(function()
        -- Mark as seen
        local seenValue = Instance.new("BoolValue")
        seenValue.Name = "SeenTelegram"
        seenValue.Value = true
        seenValue.Parent = gui
        
        -- Animate popup out
        popup:TweenSize(UDim2.new(0, 0, 0, 0), Enum.EasingDirection.In, Enum.EasingStyle.Back, 0.5, true)
        task.wait(0.5)
        popup:Destroy()
        
        -- Show icon
        showIcon()
    end)
else
    -- Already seen, show icon
    showIcon()
end

-- Function to show icon with animation
function showIcon()
    icon.Size = UDim2.new(0, 0, 0, 0)
    icon.Visible = true
    TweenService:Create(icon, TweenInfo.new(0.7, Enum.EasingDirection.Out, Enum.EasingStyle.Back), {
        Size = UDim2.new(0, 64, 0, 64)
    }):Play()
end