-- Copy function
local function copyTelegramLink()
    pcall(function()
        setclipboard("https://t.me/RBLX_SCRIPTS_MYANMAR")
    end)
    StarterGui:SetCore("SendNotification", {
        Title = "✅ Copied!",
        Text = "Telegram link copied to clipboard!",
        Duration = 4
    })
end

joinButton.MouseButton1Click:Connect(function()
    copyTelegramLink()
end)

