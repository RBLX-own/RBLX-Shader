-- Shader Toggle
local shaderButton = Instance.new("TextButton")
shaderButton.Size = UDim2.new(0.85, 0, 0.13, 0)
shaderButton.Position = UDim2.new(0.075, 0, 0.35, 0)
shaderButton.Text = "Enable Beautiful Shader"
shaderButton.BackgroundColor3 = Color3.fromRGB(0, 255, 120)
shaderButton.TextColor3 = Color3.fromRGB(0, 0, 0)
shaderButton.TextScaled = true
shaderButton.Font = Enum.Font.GothamBold
shaderButton.Parent = mainFrame

local shaderCorner = Instance.new("UICorner", shaderButton)
shaderCorner.CornerRadius = UDim.new(0, 16)
local shaderStroke = Instance.new("UIStroke", shaderButton)
shaderStroke.Color = Color3.fromRGB(255, 255, 255)
shaderStroke.Thickness = 2

local shaderEnabled = false
local function toggleShader()
    shaderEnabled = not shaderEnabled
    if shaderEnabled then
        shaderButton.Text = "Disable Shader"
        shaderButton.BackgroundColor3 = Color3.fromRGB(255, 60, 60)
        
        -- Beautiful shader effects
        local bloom = Lighting:FindFirstChild("Bloom") or Instance.new("BloomEffect")
        bloom.Enabled = true
        bloom.Intensity = 2
        bloom.Size = 40
        bloom.Threshold = 0.7
        bloom.Parent = Lighting
        
        local sunrays = Lighting:FindFirstChild("SunRays") or Instance.new("SunRaysEffect")
        sunrays.Enabled = true
        sunrays.Intensity = 0.4
        sunrays.Parent = Lighting
        
        local dof = Lighting:FindFirstChild("DepthOfField") or Instance.new("DepthOfFieldEffect")
        dof.Enabled = true
        dof.FocusDistance = 15
        dof.InFocusRadius = 30
        dof.Parent = Lighting
        
        local colorCorr = Lighting:FindFirstChild("ColorCorrection") or Instance.new("ColorCorrectionEffect")
        colorCorr.Enabled = true
        colorCorr.Brightness = 0.15
        colorCorr.Contrast = 0.3
        colorCorr.Saturation = 0.6
        colorCorr.Parent = Lighting
        
        Lighting.GlobalShadows = true
        Lighting.Brightness = 3
    else
        shaderButton.Text = "Enable Beautiful Shader"
        shaderButton.BackgroundColor3 = Color3.fromRGB(0, 255, 120)
        
        for _, v in pairs(Lighting:GetChildren()) do
            if v:IsA("BloomEffect") or v:IsA("SunRaysEffect") or v:IsA("DepthOfFieldEffect") or v:IsA("ColorCorrectionEffect") then
                v.Enabled = false
            end
        end
        Lighting.GlobalShadows = false
    end
end
shaderButton.MouseButton1Click:Connect(toggleShader)

-- FPS Boost Toggle
local fpsBoostButton = Instance.new("TextButton")
fpsBoostButton.Size = UDim2.new(0.85, 0, 0.13, 0)
fpsBoostButton.Position = UDim2.new(0.075, 0, 0.55, 0)
fpsBoostButton.Text = "Enable FPS Boost"
fpsBoostButton.BackgroundColor3 = Color3.fromRGB(0, 255, 120)
fpsBoostButton.TextColor3 = Color3.fromRGB(0, 0, 0)
fpsBoostButton.TextScaled = true
fpsBoostButton.Font = Enum.Font.GothamBold
fpsBoostButton.Parent = mainFrame

local boostCorner = Instance.new("UICorner", fpsBoostButton)
boostCorner.CornerRadius = UDim.new(0, 16)
local boostStroke = Instance.new("UIStroke", fpsBoostButton)
boostStroke.Color = Color3.fromRGB(255, 255, 255)
boostStroke.Thickness = 2

local boostEnabled = false
local function toggleFPSBoost()
    boostEnabled = not boostEnabled
    if boostEnabled then
        fpsBoostButton.Text = "Disable FPS Boost"
        fpsBoostButton.BackgroundColor3 = Color3.fromRGB(255, 60, 60)
        
        Lighting.GlobalShadows = false
        Lighting.FogEnd = 999999
        settings().Rendering.QualityLevel = Enum.QualityLevel.Level01
        
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("ParticleEmitter") or obj:IsA("Trail") or obj:IsA("Smoke") or obj:IsA("Fire") then
                obj.Enabled = false
            end
        end
    else
        fpsBoostButton.Text = "Enable FPS Boost"
        fpsBoostButton.BackgroundColor3 = Color3.fromRGB(0, 255, 120)
        Lighting.GlobalShadows = true
        settings().Rendering.QualityLevel = Enum.QualityLevel.Automatic
    end
end
fpsBoostButton.MouseButton1Click:Connect(toggleFPSBoost)

-- ===================== SQUARE ICON =====================
local squareButton = Instance.new("ImageButton")
squareButton.Size = UDim2.new(0, 90, 0, 90)
squareButton.Position = UDim2.new(0.5, -45, 0.08, 0)
squareButton.Image = "rbxassetid://121158130775447"
squareButton.BackgroundTransparency = 1
squareButton.Visible = false
squareButton.Parent = screenGui

local sqCorner = Instance.new("UICorner", squareButton)
sqCorner.CornerRadius = UDim.new(1, 0)
local sqStroke = Instance.new("UIStroke", squareButton)
sqStroke.Color = Color3.fromRGB(0, 255, 200)
sqStroke.Thickness = 6

squareButton.MouseButton1Click:Connect(function()
    mainFrame.Visible = not mainFrame.Visible
end)

-- ===================== FPS COUNTER =====================
local frames = 0
local lastTick = tick()

RunService.RenderStepped:Connect(function()
    frames += 1
    if tick() - lastTick >= 1 then
        fpsLabel.Text = "FPS: " .. frames
        frames = 0
        lastTick = tick()
    end
end)

-- ===================== CONTINUE =====================
continueButton.MouseButton1Click:Connect(function()
    telegramFrame.Visible = false
    mainFrame.Visible = true
    squareButton.Visible = true
end)

-- ===================== SOUND EFFECTS =====================
local clickSound = Instance.new("Sound")
clickSound.SoundId = "rbxassetid://12221967"
clickSound.Volume = 0.6
clickSound.Parent = screenGui

local function playClick()
    clickSound:Play()
end

-- Attach sounds
joinButton.MouseButton1Click:Connect(playClick)
continueButton.MouseButton1Click:Connect(playClick)
closeButton.MouseButton1Click:Connect(playClick)
shaderButton.MouseButton1Click:Connect(playClick)
fpsBoostButton.MouseButton1Click:Connect(playClick)
squareButton.MouseButton1Click:Connect(playClick)

-- Optional extra feature: Press "H" to hide/show square icon
UserInputService.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.H then
        squareButton.Visible = not squareButton.Visible
    end
end)

print("Myanmar Hub loaded! Enjoy the beautiful shader & FPS tools ✨")